{
  "name": "study-room-management-backend",
  "version": "1.0.0",
  "description": "Backend API for Study Room Management System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:devdb": "cross-env DB_MODE=dev NODE_ENV=development nodemon server.js",
    "start:proddb": "cross-env NODE_ENV=production nodemon server.js",
    "devdb": "npm run start:devdb",
    "proddb": "npm run start:proddb",
    "test": "echo \"Error: no test specified\" ; exit 1",
    "test:ai-chat": "node test-ai-chat.js",
    "llm:setup": "node scripts/setup-local-llm.js",
    "llm:test": "node scripts/test-local-llm.js",
    "llm:status": "node scripts/setup-local-llm.js status",
    "llm:quick": "node scripts/setup-local-llm.js quick",
    "migrate:ai-frequency": "node scripts/migrate-ai-frequency.js",
    "postinstall": "node ./scripts/build-and-copy.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "pg": "^8.11.3",
    "sql.js": "^1.13.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
